{
    "compilerOptions": {
        "target": "ES2019",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": ["ESNext", "DOM"],
        "types": ["jest", "node"],
        "allowJs": false,
        "declaration": true,
        "sourceMap": true,
        "removeComments": false,
        "strict": true,
        "rootDir": ".",
        "esModuleInterop": true,
        "composite": true,
        "forceConsistentCasingInFileNames": true
    }
}
