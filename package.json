{
    "name": "sudoku-multi-solver",
    "author": "Ot√°vio Baziewicz Filho",
    "version": "1.0.2",
    "description": "A simple and powerful sudoku solver that supports multiple game patterns up to 16x16",
    "homepage": "https://github.com/worgho2/sudoku-solver#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/worgho2/sudoku-solver.git"
    },
    "bugs": {
        "url": "https://github.com/worgho2/sudoku-solver/issues"
    },
    "license": "MIT",
    "keywords": [
        "sudoku",
        "multi",
        "solver",
        "sudoku-solver",
        "sudoku-multi-solver"
    ],
    "main": "lib/sudoku-multi-solver.js",
    "module": "lib/sudoku-multi-solver.mjs",
    "types": "lib/src/index.d.ts",
    "lint-staged": {
        "*.ts": [
            "npm run format",
            "npm run lint",
            "git add"
        ]
    },
    "files": [
        "lib",
        "pattern-models"
    ],
    "scripts": {
        "prepare": "is-ci || husky install",
        "pretest": "rimraf temp/__tests__ && tsc --project __tests__",
        "test": "jest .test.js",
        "prebuild": "rimraf lib",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "generate-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "generate-pattern-models": "ts-node scripts/generate-pattern-models.ts"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "conventional-changelog-cli": "^2.2.2",
        "eslint": "^8.34.0",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "jest": "^29.4.2",
        "lint-staged": "^13.1.1",
        "prettier": "^2.8.3",
        "rimraf": "^4.1.2",
        "rollup": "^3.15.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
